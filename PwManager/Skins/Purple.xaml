<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <FontFamily x:Key="TepenoSansRegular">pack://application:,,,/Fonts/#Tepeno Sans Regular</FontFamily>

    <Style x:Key="TextBlockStyleMain" TargetType="{x:Type TextBlock}">
        <Setter Property="TextWrapping" Value="NoWrap"/>
        <Setter Property="TextTrimming" Value="None"/>
        <Setter Property="FontFamily" Value="{StaticResource TepenoSansRegular}"/>
        <Setter Property="FontSize" Value="30"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <SolidColorBrush x:Key="ListBox.Static.Background" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ListBox.Static.Border" Color="#FFABADB3"/>
    <SolidColorBrush x:Key="ListBox.Disabled.Background" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ListBox.Disabled.Border" Color="#FFD9D9D9"/>

    <Style x:Key="FocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Rectangle Margin="2" StrokeDashArray="1 2" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" SnapsToDevicePixels="true" StrokeThickness="1"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <SolidColorBrush x:Key="AppBackgroundMainDefault" Color="#111111"/>
    <SolidColorBrush x:Key="FieldErrorDefault" Color="#c45148"/>
    <SolidColorBrush x:Key="ButtonStaticBackgroundDefault" Color="#713AC9"/>
    <SolidColorBrush x:Key="ButtonMouseOverBackgroundDefault" Color="#6132AD"/>
    <SolidColorBrush x:Key="ButtonPressedBackgroundDefault" Color="#492581"/>

    <SolidColorBrush x:Key="TextBoxStaticBackgroundDefault" Color="#5F556F"/>
    <SolidColorBrush x:Key="TextBoxStaticBorderDefault" Color="#5F556F"/>
    <SolidColorBrush x:Key="TextBoxForegroundDefault" Color="White"/>

    <SolidColorBrush x:Key="ListViewStaticBackgroundDefault" Color="#FF25212D"/>

    <SolidColorBrush x:Key="ListViewItemStaticBackgroundDefault" Color="#504660"/>
    <SolidColorBrush x:Key="ListViewItemMouseOverBackground" Color="#818694"/>
    <SolidColorBrush x:Key="ListViewItemMouseOverBorder" Color="#818694"/>
    <SolidColorBrush x:Key="ListViewItemSelectedActiveBackground" Color="#FF646875"/>
    <SolidColorBrush x:Key="ListViewItemSelectedActiveBorder" Color="#FF646875"/>




    <SolidColorBrush x:Key="Button.Static.Background" Color="#713AC9"/>
    <SolidColorBrush x:Key="Button.Static.Border" Color="#713AC9"/>
    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#6132AD"/>
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#6132AD"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#492581"/>
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#492581"/>
    <SolidColorBrush x:Key="Button.Disabled.Background" Color="DarkGray"/>
    <SolidColorBrush x:Key="Button.Disabled.Border" Color="DarkGray"/>
    <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="Black"/>

    <SolidColorBrush x:Key="TextBox.Static.Border" Color="#FFABAdB3"/>
    <SolidColorBrush x:Key="TextBox.MouseOver.Border" Color="#FF7EB4EA"/>
    <SolidColorBrush x:Key="TextBox.Focus.Border" Color="#FF569DE5"/>



    <Style x:Key="TextBoxStyleMain" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="{DynamicResource TextBoxStaticBackgroundDefault}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource TextBoxStaticBorderDefault}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextBoxForegroundDefault}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="FontSize" Value="25"/>
        <Setter Property="FontFamily" Value="{StaticResource TepenoSansRegular}"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Cycle"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="AllowDrop" Value="true"/>
        <Setter Property="ScrollViewer.PanningMode" Value="HorizontalOnly"/>
        <Setter Property="Stylus.IsFlicksEnabled" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="5" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="True">
                        <ScrollViewer x:Name="PART_ContentHost" Focusable="false" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Opacity" TargetName="border" Value="0.56"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource TextBox.MouseOver.Border}"/>
                        </Trigger>
                        <Trigger Property="IsKeyboardFocused" Value="true">
                            <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource TextBox.Focus.Border}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsInactiveSelectionHighlightEnabled" Value="true"/>
                    <Condition Property="IsSelectionActive" Value="false"/>
                </MultiTrigger.Conditions>
                <Setter Property="SelectionBrush" Value="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
            </MultiTrigger>
        </Style.Triggers>
    </Style>


    <Style x:Key="TextBoxStyleMainClipboard" TargetType="{x:Type TextBox}">
